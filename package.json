{
  "name": "nextjs-landing-starter",
  "version": "2.5.1",
  "description": "Common landing website starter driven by next.js",
  "main": "src/server/app.js",
  "author": "Paralect",
  "license": "MIT",
  "keywords": [
    "landing",
    "next.js",
    "react",
    "postcss",
    "paralect"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/paralect/nextjs-landing-starter"
  },
  "scripts": {
    "build": "next build src/client",
    "development": "npm run next-server",
    "start": "NODE_ENV=production npm run next-server",
    "next-server": "node ./src/server/app.js",
    "test": "run-s test:**",
    "test:eslint": "eslint  --ext js --ext jsx ./src/",
    "test:lint-styles": "stylelint \"src/client/**/*.pcss\"",
    "add-contributor": "all-contributors add",
    "generate-contributors": "all-contributors generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --ext js --ext jsx",
      "git add"
    ],
    "*.pcss": [
      "stylelint",
      "git add"
    ]
  },
  "dependencies": {
    "@paralect/common-logger": "0.1.10",
    "acorn": "6.4.1",
    "axios": "0.19.0",
    "classnames": "2.2.6",
    "cssnano": "4.1.10",
    "find-up": "4.1.0",
    "lodash": "4.17.15",
    "mini-css-extract-plugin": "0.8.0",
    "next": "9.0.2",
    "normalize.css": "8.0.1",
    "npm-run-all": "4.1.5",
    "postcss-import": "12.0.1",
    "postcss-nested": "4.1.2",
    "postcss-preset-env": "6.7.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-icon-base": "2.1.2",
    "stylelint": "10.1.0",
    "stylelint-config-standard": "18.3.0"
  },
  "devDependencies": {
    "all-contributors-cli": "6.8.1",
    "babel-eslint": "10.0.3",
    "babel-plugin-inline-react-svg": "1.1.0",
    "babel-plugin-module-resolver": "3.2.0",
    "css-loader": "3.1.0",
    "eslint": "6.7.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-import-resolver-custom-alias": "1.2.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.17.0",
    "extracted-loader": "1.0.7",
    "husky": "4.0.0-beta.5",
    "lint-staged": "9.5.0",
    "postcss-loader": "3.0.0"
  }
}
